{
    "manifest_version": 3,
    "name": "Password Manager Extension",
    "version": "1.0",
    "action":
    {
        "default_popup": "password.html",
        "default_icon": "hello_extensions.png"
    },
    "background":
    {
        "service_worker": "background.js"     
    },
    "web_accessible_resources": [ {
        "resources": ["happy.png"],
        "matches": ["http://*/*", "https://*/*"]
    }],
    "permissions": ["tabs", "notifications", "storage", "webNavigation", "scripting", "activeTab", "contextMenus"],
    "host_permissions": ["http://*/*", "https://*/*"]
 }