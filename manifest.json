{
    "manifest_version": 3,
    "name": "Hello Extensions",
    "version": "1.0",
    "background":
    {
        "service_worker": "background.js"     
    },
    "action":
    {
        "default_popup": "password.html",
        "default_icon": "hello_extensions.png"
    },
    "permissions": ["tabs", "notifications", "storage", "webNavigation", "scripting"],
    "host_permissions": ["http://*/*", "https://*/*"]
 }